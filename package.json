{
  "name": "prilabsa-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Prilabsa Corporate Website 2025 - Built with Solaria Agency Framework",
  "keywords": [
    "prilabsa",
    "corporate-website",
    "react",
    "typescript",
    "vite",
    "tailwindcss"
  ],
  "author": {
    "name": "Prilabsa",
    "email": "info@prilabsa.com",
    "url": "https://prilabsa.com"
  },
  "homepage": "https://prilabsa.com",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite --port 5174",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "audit:security": "npm audit --audit-level critical --json",
    "audit:fix": "npm audit fix",
    "compose:report": "tsx scripts/composeReport.ts",
    "predeploy": "npm run test:run && npm run lint && npm run build",
    "deploy": "echo 'Deployment ready'",
    "clean": "rm -rf dist coverage node_modules/.cache",
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "prepare": "husky"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.10",
    "@tanstack/react-query": "^5.17.0",
    "@types/react-modal": "^3.16.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-slick": "^0.23.13",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-modal": "^3.16.3",
    "react-router-dom": "^7.6.2",
    "react-slick": "^0.30.3",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@mendable/firecrawl-js": "^1.25.5",
    "@netlify/functions": "^4.1.2",
    "@playwright/test": "^1.53.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.0.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "^3.2.3",
    "autoprefixer": "^10.4.16",
    "cypress": "^14.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "playwright": "^1.53.0",
    "postcss": "^8.4.34",
    "puppeteer": "^24.10.0",
    "tailwindcss": "^4.1.10",
    "tsx": "^4.20.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.3"
  }
}
